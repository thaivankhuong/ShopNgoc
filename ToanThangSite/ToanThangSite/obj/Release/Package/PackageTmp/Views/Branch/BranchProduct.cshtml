@model IEnumerable<ToanThangSite.Entities.Core.Branch>
@foreach (var item in Model)
{
    <div class="item">
        <div class="map-1">
            <p><b>@item.Title </b><span>Click để xem bản đồ</span></p>
        </div>
        <div class="map-2">
            <iframe style="border:0;width:100%;height:450px" src="@item.Map" allowfullscreen=""></iframe>
        </div>
    </div>
}
